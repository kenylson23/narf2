{
  "redirects": [
    {
      "from": "/*",
      "to": "/index.html",
      "status": 200
    },
    {
      "from": "/api/*",
      "to": "https://api.colegionarfive.ao/:splat",
      "status": 200,
      "force": true
    },
    {
      "from": "/assets/*",
      "to": "/assets/:splat",
      "status": 200,
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ]
}
